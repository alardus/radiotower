{
  "manifest_version": 2,

  "name": "Portaller Radio Tower",
  "description": "Listen to Pandora and Spotify outside U.S.",
  "version": "1.0",

  "icons": { "16": "icon.png",
           "48": "icon48.png",
          "128": "icon128.png" },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "proxy"
  ]
}
